<p id="notice"><%= notice %></p>

<h1>News Logs</h1>

<table>
  <thead>
    <tr>
      <th>Received date</th>
      <th>Release date</th>
      <th>Title</th>
      <th>User</th>
      <th>Agency</th>
      <th>Region</th>
      <th>Distributionlist</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @news_logs.each do |news_log| %>
      <tr>
        <td><%= news_log.received_date %></td>
        <td><%= news_log.release_date %></td>
        <td><%= news_log.title %></td>
        <td><%= news_log.user %></td>
        <td><%= news_log.agency %></td>
        <td><%= news_log.region %></td>
        <td><%= news_log.distributionlist %></td>
        <td><%= link_to 'Show', news_log %></td>
        <td><%= link_to 'Edit', edit_news_log_path(news_log) %></td>
        <td><%= link_to 'Destroy', news_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New News Log', new_news_log_path %>
