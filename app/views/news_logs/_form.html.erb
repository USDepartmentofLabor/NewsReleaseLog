<%= semantic_form_for news_log do |f| %>
    <%= f.inputs do %>
      <%= f.input :title %>
      <%= f.input :region, :as => :select, :collection => @region_hash %>
      <%= f.input :agency, :as => :select, :collection => @agency_hash %>
      <%= f.input :received_date, input_html: {  type: 'text', class: 'datepicker'}%>
      <%= f.input :release_date, input_html: { type: 'text', class: 'datepicker' }%>
      <%= f.input :distributionlists, :as => :select, :collection => @distributionlist_hash %>
    <% end %>
    <%= f.actions do %>
      <%= f.action :submit, :as => :button , :input_html => {:class => "btn btn-primary"}%>
      <%= f.action :cancel, :as => :link , :url => "/" %>
    <% end %>
  <% end %>
